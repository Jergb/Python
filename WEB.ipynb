    "from bottle import route, run\n",
    "\n",
    "@route('/')\n",
    "@route('/hello/<name>')\n",
    "def greet(name='RULES!!!'):\n",
    "    return 'PYTHON %s   :D'%name\n",
    "\n",
    "run(host='192.168.101.41', port=8080, debug=True)"
